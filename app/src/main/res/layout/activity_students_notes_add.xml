<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".studentsnotes.StudentsNotesAdd">

    <EditText
        android:id="@+id/editTextPhone"
        android:layout_width="311dp"
        android:layout_height="49dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="188dp"
        android:backgroundTint="@color/black"
        android:ems="10"
        android:inputType="phone"
        android:text="@string/subTite"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextPhone6"
        android:layout_width="306dp"
        android:layout_height="48dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="296dp"
        android:layout_marginEnd="45dp"
        android:layout_marginBottom="52dp"
        android:backgroundTint="@color/black"
        android:ems="10"
        android:inputType="phone"
        android:text="@string/subDis"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextPhone7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextPhone7"
        android:layout_width="304dp"
        android:layout_height="48dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="52dp"
        android:backgroundTint="@color/black"
        android:ems="10"
        android:inputType="phone"
        android:text="@string/imgUrl"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextPhone6" />

    <Button
        android:id="@+id/button2"
        android:layout_width="146dp"
        android:layout_height="48dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="72dp"
        android:backgroundTint="@color/btn_backgroundcolor"
        android:text="@string/save"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextPhone7" />

    <Button
        android:id="@+id/button5"
        android:layout_width="93dp"
        android:layout_height="46dp"
        android:layout_marginStart="156dp"
        android:layout_marginTop="32dp"
        android:backgroundTint="@color/btn_backgroundcolor"
        android:text="@string/back"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        tools:text="@string/back" />

</androidx.constraintlayout.widget.ConstraintLayout>